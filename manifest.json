{

  "description": "Chacks GMail account using atom feed",
  "manifest_version": 2,
  "name": "gmail-notifier",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
 
  "browser_action": {
    "default_icon":"icons/gmail-64.png",
    "default_title":"Gmail Notifier",
    "browser_style":false
  },
  "permissions": ["alarms", "*://mail.google.com/mail/*", "tabs", "contextMenus"],
  "web_accessible_resources": [
    "icons/gmail-64.png",
    "icons/gmail-64-unread.png"
  ]
}
